<Ui xmlns="http://www.blizzard.com/wow/ui/"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <Frame name="RazyStatusBar_MainFrame" frameStrata="HIGH" toplevel="true" enableMouse="true" movable="true" parent="UIParent">
        <Size x="170" y="20" />
        <Anchors>
            <Anchor point="CENTER"/>
        </Anchors>
        <Backdrop edgeFile="Interface\Tooltips\UI-Tooltip-Border" bgFile="Interface\Tooltips\UI-Tooltip-Background" tile="true">
            <EdgeSize>
                <AbsValue val="16" />
            </EdgeSize>
            <TileSize>
                <AbsValue val="16" />
            </TileSize>
            <BackgroundInsets>
                <AbsInset left="5" right="5" top="5" bottom="5" />
            </BackgroundInsets>
        </Backdrop>
        <Script>
            <OnLoad>
                RazyStatusBar_OnLoad();
            </OnLoad>
        </Script>
    </Frame>
</Ui>
